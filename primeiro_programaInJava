public class MinhaCalc {
    public static int multiplicar(int a, int b) {
        int x;
        x = a * b;
        return x;
    }
    public static void main(String[] args) {
        int x = 4, y = 5, z;
        System.out.println("Calculadora");
        z = multiplicar(x, y);
        System.out.println("z = " + z);
        System.out.println("z = " + multiplicar(10, 3));
    }
}
